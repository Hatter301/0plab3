#include <stdio.h>
int main()
{
    int i,j;
    float s,a[3][4];
    printf("Введите массив\n");
    for (i=0;i<3;i++)
    for (j=0;j<4;j++)
    scanf("%f",&a[i][j]);
    printf("Введите скаляр\n");
    scanf("%f",&s);
    for (i=0;i<3;i++)
    for (j=0;j<4;j++)
    a[i][j]=a[i][j]*s;
    for (i=0;i<3;i++) {
    for (j=0;j<4;j++) {
    printf("%.2f ",a[i][j]);
    }
    printf("\n");
    }
    return 0;
}
