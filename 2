#include <stdio.h>
main ()
{
    int i;
    char s[100];
    int a[255];
    int b[255];
    for(i=0;i!=256;i++)
    a[i]=0;
    for(i=0;i!=256;i++)
    b[i]=0;
    gets(s);
    for (i=0;s[i]!='\0';i++) 
        if ((((int)s[i]>=65) && ((int)s[i]<=90))||(((int)s[i]>=97) && ((int)s[i]<=122))) 
    a[(int)s[i]]++;
     for (i=0;s[i]!='\0';i++) 
        if ((((int)s[i]>=65) && ((int)s[i]<=90))||(((int)s[i]>=97) && ((int)s[i]<=122))) 
            if(b[(int)s[i]]==0) {
            printf("%c ",s[i]);
            printf("%d\n",a[(int)s[i]]);
            b[(int)s[i]]=1;
            }
return 0;
}
