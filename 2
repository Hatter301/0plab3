#include <stdio.h>
main ()
{
    int i,f=1,h=0,j,g;
    char s[100];
    gets(s);
    for (i=0;s[i]!='\0';i++) {
        if ((((int)s[i]>=65) && ((int)s[i]<=90))||(((int)s[i]>=97) && ((int)s[i]<=122))) {
    for (j=i-1;j!=-1;j-=1)
    if (s[i]==s[j])
    h=1;
    if (h==1) {
        h=0;
    continue;
    }
    else {
    for(g=i+1;s[g]!='\0';g++) 
    if (s[i]==s[g])
    f+=1;
    printf("%c ",s[i]);
    printf("%d\n",f);
    f=1;
    h=0;
    }
        }
    }
return 0;
}
